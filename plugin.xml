<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="MongoWorkBench.category1"
            name="MongoWorkBench">
      </category>
      <view
            allowMultiple="false"
            category="MongoWorkBench.category1"
            class="org.aw20.mongoworkbench.eclipse.view.MDBTree"
            icon="src/org/aw20/mongoworkbench/eclipse/resources/database_gear.png"
            id="org.aw20.mongoworkbench.eclipse.view.MDBTree"
            name="DB Tree"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="MongoWorkBench.category1"
            class="org.aw20.mongoworkbench.eclipse.view.MCommandConsole"
            icon="src/org/aw20/mongoworkbench/eclipse/resources/cog_start.png"
            id="org.aw20.mongoworkbench.eclipse.view.MCommandConsole"
            name="Cmd Console"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="MongoWorkBench.category1"
            class="org.aw20.mongoworkbench.eclipse.view.MCommandWizardView"
            icon="src/org/aw20/mongoworkbench/eclipse/resources/cog_start.png"
            id="org.aw20.mongoworkbench.eclipse.view.MCommandWizardView"
            name="Cmd Wizards"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="MongoWorkBench.category1"
            class="org.aw20.mongoworkbench.eclipse.view.MHistoryView"
            icon="src/org/aw20/mongoworkbench/eclipse/resources/cog_start.png"
            id="org.aw20.mongoworkbench.eclipse.view.MHistoryView"
            name="Cmd History"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="MongoWorkBench.category1"
            class="org.aw20.mongoworkbench.eclipse.view.MDBShowStatistics"
            icon="src/org/aw20/mongoworkbench/eclipse/resources/database.png"
            id="org.aw20.mongoworkbench.eclipse.view.MDBShowStatistics"
            name="Server Statistics"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="MongoWorkBench.category1"
            class="org.aw20.mongoworkbench.eclipse.view.MCollectionShowStatus"
            icon="src/org/aw20/mongoworkbench/eclipse/resources/table_multiple.png"
            id="org.aw20.mongoworkbench.eclipse.view.MCollectionShowStatus"
            name="Collection List"
            restorable="true">
      </view>
      <view
            category="MongoWorkBench.category1"
            class="org.aw20.mongoworkbench.eclipse.view.MDocumentView"
            icon="src/org/aw20/mongoworkbench/eclipse/resources/table.png"
            id="org.aw20.mongoworkbench.eclipse.view.MDocumentView"
            name="Query Output"
            restorable="true">
      </view>
      <view
            category="MongoWorkBench.category1"
            class="org.aw20.mongoworkbench.eclipse.view.MJavaScriptView"
            icon="src/org/aw20/mongoworkbench/eclipse/resources/database_go.png"
            id="org.aw20.mongoworkbench.eclipse.view.MJavaScriptView"
            name="JavaScript"
            restorable="true">
      </view>
     <view
            category="MongoWorkBench.category1"
            class="org.aw20.mongoworkbench.eclipse.view.MEditor"
            icon="src/org/aw20/mongoworkbench/eclipse/resources/table_edit.png"
            id="org.aw20.mongoworkbench.eclipse.view.MEditor"
            name="Editor"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.aw20.mongoworkbench.eclipse.pref.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.aw20.mongoworkbench.eclipse.pref.MPrefPage"
            id="org.aw20.mongoworkbench.eclipse.pref.MPrefPage"
            name="MongoWorkBench">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="MongoWorkBench.TopMenu"
            name="MongoWorkBench">
      </category>
      <command
            categoryId="MongoWorkBench.TopMenu"
            defaultHandler="org.aw20.mongoworkbench.eclipse.handler.MActivationHandler"
            id="MongoWorkBench.activateCommand"
            name="Open MongoWorkBench Perspective">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="MongoWorkBench">
            <command
                  commandId="MongoWorkBench.activateCommand"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.aw20.mongoworkbench.eclipse.MPerspectiveFactory1"
            icon="src/org/aw20/mongoworkbench/eclipse/resources/database_gear.png"
            id="org.aw20.mongoworkbench.eclipse.MPerspectiveFactory1"
            name="MongoWorkbench">
      </perspective>
   </extension>

</plugin>
